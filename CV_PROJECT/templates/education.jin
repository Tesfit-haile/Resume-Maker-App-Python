{% extends "base.jin" %}{% block body %}    <div class="container">        <form method="POST" action="" novalidate>         <fieldset class="form-group mt-5">             <h4 class="text-center"> Education</h4>            {{  educationForm.hidden_tag() }}        <div class="row">                <div class="col-7">                    <p>                            {{ educationForm.education.label }}                            {{ educationForm.education(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.education.errors[0] }}</span>                        </p>                    <p>                            {{ educationForm.university.label }}                            {{ educationForm.university(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.university.errors[0] }}</span>                        </p>                    <p>                            {{ educationForm.graduate_date.label }}                            {{ educationForm.graduate_date(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.graduate_date.errors[0] }}</span>                        </p>                        <p>                            {{ educationForm.education_description.label }}                            {{ educationForm.education_description(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.education_description.errors[0] }}</span>                        </p>                    <p class="form-group">                        {{  educationForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div><!--             <div class="col-1"></div>-->                    <div class="col-4">                        <h4 class="text-center"> Info </h4>                        <p class="lead text-center">                            adipisicing elit. Neque iure temporibus                            eligendi cum dolorem excepturi placeat,                            necessitatibus sequi quasi labore voluptate,                            necessitatibus sequi quasi labore voluptate                        </p>                    </div>        </div>     <a href="/workExperience"><button type="button" class="btn btn-primary m-3">Previous</button></a>     <a href="/project"><button type="button" class="btn btn-primary float-right m-3">Next Page</button></a>             </fieldset>        </form>    </div>{% endblock %}