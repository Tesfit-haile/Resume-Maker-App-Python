{% extends 'base.jin' %}{% block body %}    <img src="https://upload.wikimedia.org/wikipedia/commons/1/1e/Bald_Eagle_Portrait.jpg" alt="..." style="width: 100px; height: 100px; border-radius: 50%; margin-left: 40%; margin-top: 22px;"><br><br>                            {# user name #}    <h1 class="text-center mt-4">{{ user_obj.username }}</h1>    <h4 class="text-center mt-4">{{ user_obj.email }} - {{ user_obj.city }}</h4>    <hr style="border: 1px solid black; margin-top: 10%;">    <h3 class="text-center" style="background-color: rgb(245, 247, 252);">Profile<br>    <hr style="border: 1px solid black; margin-top: 1px; width: 80px"></h3>    <p style="margin-left: 10%;">{{ user_obj.description }}</p>    <hr style="border: 1px solid black;">                            {# user projects #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)">Projects<br>    <hr style="border: 1px solid black; margin-top: 1px; width: 110px"></h3>    {% for project in all_projects %}        <div class="row">                <div class="col-2"></div>                <div class="col-10"><li>{{ project.project }}</li></div>        </div>    {% endfor %}                        {# user Experience #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)">Experience <br>    <hr style="border: 1px solid black; margin-top: 1px; width: 140px"></h3>    <ol>       {% for exp in all_experience %}           <div class="row">           <div class="col-2"></div>           <div class="col-6"><li><h5><i>{{ exp.title }}</i> </h5> {{ exp.work }}</li></div>           <div class="col-4">{{ exp.entery_date }} -------- <span>{{ exp.exit_date }}</span></div>           </div><br>            {# ol shoud be outside the loop #}        {% endfor %}    </ol>                                {# education #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)">Education<br>    <hr style="border: 1px solid black; margin-top: 1px; width: 120px"></h3>    {% for edu in all_education %}           <div class="row">           <div class="col-2"></div>           <div class="col-10"><li>{{ edu.education }}</li></div>           </div>            {# ol shoud be outside the loop #}        {% endfor %}                                    {# Skill #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)"> Skills<br>    <hr style="border: 1px solid black; margin-top: 1px; width: 70px"></h3>    {% for skill in all_skills %}           <div class="row">           <div class="col-2"></div>           <div class="col-10"><li>{{ skill.skill }}</li> </div>           </div>            {# ol shoud be outside the loop #}        {% endfor %}                                        {# languages #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)"> Languages <br>        <hr style="border: 1px solid black; margin-top: 1px; width: 140px"></h3>    {% for lan in all_languages %}           <div class="row">           <div class="col-2"></div>           <div class="col-10"><li>{{ lan.language }}</li> </div>           </div>            {# ol shoud be outside the loop #}        {% endfor %}                            {# tools #}    <h3 class="text-center" style="background-color: rgb(245, 247, 252)"> Tools <br>        <hr style="border: 1px solid black; margin-top: 1px; width: 70px"></h3>{% endblock body %}