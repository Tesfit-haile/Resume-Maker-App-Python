{% extends "base.jin" %}{% block body %}    <div class="container">        <form method="POST" action="" novalidate>         <fieldset class="form-group mt-5">             <h4 class="text-center"> Skill </h4>            {{  skillForm.hidden_tag() }}              <div class="row">                <div class="col-7">                        <p>                            {{ skillForm.skill.label }}                            {{ skillForm.skill(class="form-control form-control-m")}}                            <span style="color:red">{{ skillForm.skill.errors[0] }}</span>                        </p>                        <p>                            {{ skillForm.skill_description.label }}                            {{ skillForm.skill_description(class="form-control form-control-m")}}                            <span style="color:red">{{ skillForm.skill_description.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                        {{  skillForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>                   <div class="col-1"></div>                    <div class="col-4">                        <h4 class="text-center"> Info </h4>                        <p class="lead text-center">                            adipisicing elit. Neque iure temporibus                            eligendi cum dolorem excepturi placeat,                            necessitatibus sequi quasi labore voluptate,                        </p>                    </div>                </div>             <a href="/language"><button type="button" class="btn btn-primary m-3">Previous</button></a>             </fieldset>        </form>    </div>{% endblock %}