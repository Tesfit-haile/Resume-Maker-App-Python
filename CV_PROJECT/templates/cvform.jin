{% extends "base.jin" %}{% block body %}    <div class="container">        <form method="POST" action="" novalidate>         <fieldset class="form-group mt-5">             <h4 class="text-center"> Your CV </h4>            {{  userCvForms.hidden_tag() }}        <div class="row">                <div class="col-6">                        <p>                            {{ userCvForms.username.label(class="form-control-label") }}                            {{ userCvForms.username(class="form-control form-control-m") }}                            <span style="color:red">{{ userCvForms.username.errors[0] }}</span>                        </p>                        <p>                            {{ userCvForms.fathername.label(class="form-control-label") }}                            {{ userCvForms.fathername(class="form-control form-control-m")}}                            <span style="color:red">{{ userCvForms.fathername.errors[0] }}</span>                        </p>                        <p>                            {{ userCvForms.email.label(class="form-control-label") }}                            {{ userCvForms.email(class="form-control form-control-m") }}                            <span style="color:red">{{ userCvForms.email.errors[0] }}</span>                        </p>                        <p>                           {{userCvForms.age.label(class="form-control-label")  }}                            {{ userCvForms.age(class="form-control form-control-m") }}                            <span style="color:red">{{ userCvForms.age.errors[0] }}</span>                        </p>                        <p>                           {{userCvForms.city.label(class="form-control-label") }}                           {{ userCvForms.city(class="form-control form-control-m") }}                            <span style="color:red">{{ userCvForms.city.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                        {{  userCvForms.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>{# 1st col ends here #}                <div class="col-6">                    <p>                            {{ workForm.title.label }}                            {{ workForm.title(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.title.errors[0] }}</span>                        </p>                        <p>                            {{ workForm.work.label }}                            {{ workForm.work(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.work.errors[0] }}</span>                        </p>                        <p>                            {{ workForm.entery_date.label }}                            {{ workForm.entery_date(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.entery_date.errors[0] }}</span>                        </p>                        <p>                           {{workForm.exit_date.label  }}                            {{ workForm.exit_date(class="form-control form-control-m")}}                            <span style="color:red">{{ workForm.exit_date.errors[0] }}</span>                        </p>                        <p>                           {{workForm.experience_description.label }}                           {{ workForm.experience_description(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.experience_description.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                        {{  workForm.submit(class="btn btn-outline-info m-auto") }}                     </p>                </div> {# 2nd col ends here #}            </div>{# 1s row ends here #}{# ============================================================================= #}<br><hr style="margin-top: 30px">            <div class="row mt-5">                <div class="col-3">                     <p>                            {{ projectForm.project.label }}                            {{ projectForm.project(class="form-control form-control-m")}}                            <span style="color:red">{{ projectForm.project.errors[0] }}</span>                        </p>                        <p>                            {{ projectForm.project_description.label }}                            {{ projectForm.project_description(class="form-control form-control-m") }}                            <span style="color:red">{{ projectForm.project_description.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                     {{  projectForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>                <hr style="border: 1px dashed green; height: 155px; margin-left: auto">                <div class="col-3">                    <p>                            {{ educationForm.education.label }}                            {{ educationForm.education(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.education.errors[0] }}</span>                        </p>                    <p>                            {{ educationForm.university.label }}                            {{ educationForm.university(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.university.errors[0] }}</span>                        </p>                    <p>                            {{ educationForm.graduate_date.label }}                            {{ educationForm.graduate_date(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.graduate_date.errors[0] }}</span>                        </p>                        <p>                            {{ educationForm.education_description.label }}                            {{ educationForm.education_description(class="form-control form-control-m") }}                            <span style="color:red">{{ educationForm.education_description.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                        {{  educationForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>                <hr style="border: 1px dashed green; height: 155px; margin-left: auto">                <div class="col-3">                        <p>                            {{ skillForm.skill.label }}                            {{ skillForm.skill(class="form-control form-control-m")}}                            <span style="color:red">{{ skillForm.skill.errors[0] }}</span>                        </p>                        <p>                            {{ skillForm.skill_description.label }}                            {{ skillForm.skill_description(class="form-control form-control-m")}}                            <span style="color:red">{{ skillForm.skill_description.errors[0] }}</span>                        </p>                    <p class="form-group text-center">                        {{  skillForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>                <div class="col-3">                        <p>                            {{ languageForm.language.label }}                            {{ languageForm.language(class="form-control form-control-m")}}                            <span style="color:red">{{ languageForm.language.errors[0] }}</span>                        </p>                </div>            </div>             <p class="form-group text-center">                {{  languageForm.submit(class="btn btn-outline-info m-auto") }}            </p>             </fieldset>        </form>    </div>{% endblock %}