{% extends "base.jin" %}{% block body %}    <div class="container">        <form method="POST" action="" novalidate>         <fieldset class="form-group mt-5">             <h4 class="text-center"> Work-Experience </h4>            {{  workForm.hidden_tag() }}        <div class="row">                <div class="col-7">                    <p>                            {{ workForm.title.label }}                            {{ workForm.title(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.title.errors[0] }}</span>                        </p>                        <p>                            {{ workForm.work.label }}                            {{ workForm.work(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.work.errors[0] }}</span>                        </p>                        <p>                            {{ workForm.entery_date.label }}                            {{ workForm.entery_date(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.entery_date.errors[0] }}</span>                        </p>                        <p>                           {{workForm.exit_date.label  }}                            {{ workForm.exit_date(class="form-control form-control-m")}}                            <span style="color:red">{{ workForm.exit_date.errors[0] }}</span>                        </p>                        <p>                           {{workForm.experience_description.label }}                           {{ workForm.experience_description(class="form-control form-control-m") }}                            <span style="color:red">{{ workForm.experience_description.errors[0] }}</span>                        </p>                    <p class="form-group">                        {{  workForm.submit(class="btn btn-outline-info m-auto") }}                     </p>                </div> {# 2nd col ends here #}                     <div class="col-1"></div>                    <div class="col-4">                        <h4 class="text-center"> Info </h4>                        <p class="lead text-center">                            adipisicing elit. Neque iure temporibus                            eligendi cum dolorem excepturi placeat,                            necessitatibus sequi quasi labore voluptate,                        </p>                    </div>            </div>{# 1s row ends here #}               <a href="/userAddress"><button type="button" class="btn btn-primary m-3">Previous</button></a>               <a href="/education"><button type="button" class="btn btn-primary float-right m-3">Next page</button></a>             </fieldset>        </form>    </div>{% endblock %}