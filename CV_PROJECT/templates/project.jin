{% extends "base.jin" %}{% block body %}    <div class="container">        <form method="POST" action="" novalidate>         <fieldset class="form-group mt-5">             <h4 class="text-center"> Project </h4>            {{  projectForm.hidden_tag() }}            <br><hr style="margin-top: 30px">            <div class="row mt-5">                <div class="col-7">                     <p>                            {{ projectForm.project.label }}                            {{ projectForm.project(class="form-control form-control-m")}}                            <span style="color:red">{{ projectForm.project.errors[0] }}</span>                        </p>                        <p>                            {{ projectForm.project_description.label }}                            {{ projectForm.project_description(class="form-control form-control-m") }}                            <span style="color:red">{{ projectForm.project_description.errors[0] }}</span>                        </p>                    <p class="form-group">                     {{  projectForm.submit(class="btn btn-outline-info m-auto") }}                    </p>                </div>                 <div class="col-1"></div>                    <div class="col-4">                        <h4 class="text-center"> Info </h4>                        <p class="lead text-center">                            adipisicing elit. Neque iure temporibus                            eligendi cum dolorem excepturi placeat,                            necessitatibus sequi quasi labore voluptate,                        </p>                    </div>            </div>               <a href="/education"><button type="button" class="btn btn-primary m-3">Previous</button></a>               <a href="/language"><button type="button" class="btn btn-primary float-right m-3">Next Page</button></a>             </fieldset>        </form>    </div>{% endblock %}